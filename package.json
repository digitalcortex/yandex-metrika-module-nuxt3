{
  "name": "yandex-metrika-module-nuxt3",
  "version": "1.4.0",
  "description": "Add Yandex Metrika to your Nuxt 3 application.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalcortex/yandex-metrika-module-nuxt3.git"
  },
  "homepage": "digitalcortex/yandex-metrika-module-nuxt3",
  "bugs": {
    "url": "https://github.com/digitalcortex/yandex-metrika-module-nuxt3/issues"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./dist/runtime/*": "./dist/runtime/*",
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:setup": "nuxt-module-build --stub && nuxi prepare playground",
    "prepack": "yarn build",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@release-it/conventional-changelog": "^4.2.0",
    "@types/node": "latest",
    "eslint": "latest",
    "nuxt3": "latest",
    "release-it": "^14.12.5"
  },
  "packageManager": "yarn@3.2.0"
}
